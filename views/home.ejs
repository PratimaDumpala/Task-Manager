
<!DOCTYPE html>
<html>
	<head>
		<title>Processes</title>
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" 				integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
		<link href="/stylesheets/main.css" rel="stylesheet">
	</head>
<body>
<nav class="navbar navbar-default navbar-inverse">
			<div class="container-fluid">
				<div class="navar-header" style="color:white;">
					<a class="navbar-brand style="color:white;" href="/">Processes</a>			
				</div>
				<div class="collapse navbar-collapse">
					<ul class="nav navbar-right navbar-nav">
						
						    
							
					
					</ul>			
				</div>
			</div>
		</nav>
	<h3 style="text-align:center">REMOTE TASK MANAGER</h3>
	<br>
	<br>

	<ul>
	
	<table class="table table-striped">
		<tr>
			<td style="width:25%; text-align:left;" ><strong>NAME</strong></td>
			<td style="width:20%; text-align:left;"><strong>PID</strong></td>
			<td style="width:15%; text-align:left;"><strong>START TIME</strong></td>
			<td style="width:20%; text-align:left;"><strong>CPU UTILIZATION<strong></td>
			<td style="width:15%; text-align:left;"><strong>PRIORITY</strong></td>
			<td style="width:5%; text-align:left;"><strong>KILL<strong></td>
		</tr>
	
	<% tasks.forEach(function( task){ %>
	
		<tr>
			<td style="width:25%; text-align:left;"><%=task.name%></td>
			<td style="width:20%; text-align:left;"><%=task.pid%></td>
			<td style="width:15%; text-align:left;"><%=task.starttime.toDateString()%></td>
			<td style="width:20%; text-align:left;"><%=task.cpu%></td>
			<td style="width:15%; text-align:left;"><%=task.priority%></td>
			<td style="width:5%; text-align:left;"><a href="/kill/<%=task.pid%>" class="btn btn-danger btn-xs">Kill</a></td>
		</tr>
		
		
	<% }) %>
	</table>

	</ul>
</div>


<script type="text/javascript" src="process.js"></script>
</body>
</html>





